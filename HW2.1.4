using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace HW2._1._4
{
    class Program
    {
        static void Main(string[] args)
        {
            bool isInt;
            int a, b;
            Console.WriteLine("Enter numbers A and B. (A < B)");
            do
            {
                Console.Write("A: ");
                isInt = int.TryParse(Console.ReadLine(), out a);
                if (!isInt) Console.WriteLine("The number isn't correct!");
            } while (!isInt);
            do
            {
                Console.Write("B: ");
                isInt = int.TryParse(Console.ReadLine(), out b);
                if (!isInt || b < a) Console.WriteLine("The number isn't correct!");
            } while (!isInt || b < a);
            for (; a <= b; a++)
            {
                for (int i = 0; i < a; i++)
                {
                    Console.Write(a + " ");
                }
                Console.WriteLine();
            }
        }
    }
}
