using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace HW2._1._5
{
    class Program
    {
        static void Main(string[] args)
        {
            int n;
            string str;
            Console.WriteLine("Enter your number: ");
            str = Console.ReadLine();
            bool isInt = int.TryParse(str, out n);
            Console.WriteLine("Your inverted number: ");
            Console.Write(n % 10);          
            while ((n /= 10) != 0)
            {
                Console.Write(n % 10);
            }
            Console.WriteLine();
        }
    }
}
